{
  "name": "markdown-it-toc-and-anchor",
  "version": "3.0.1",
  "description": "markdown-it plugin to add toc and anchor links in headings",
  "keywords": [
    "makrdown-it",
    "makrdown-it-plugin",
    "makrdown",
    "toc",
    "anchor"
  ],
  "author": "Maxime Thirouin",
  "license": "MIT",
  "repository": "https://github.com/MoOx/markdown-it-toc-and-anchor.git",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src",
    "!**/__tests__"
  ],
  "dependencies": {
    "uslug": "^1.0.3"
  },
  "devDependencies": {
    "ava": "^0.10.0",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-i-am-meticulous": "^1.0.0",
    "markdown-it": "^5.0.0",
    "npmpub": "^3.1.0"
  },
  "scripts": {
    "lint": "eslint ./src/",
    "tests": "ava",
    "test": "npm run lint && npm run tests",
    "prepublish": "babel --ignore __tests__ src --out-dir dist",
    "release": "npmpub"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-i-am-meticulous"
    ]
  },
  "ava": {
    "files": [
      "**/__tests__/*.js"
    ],
    "require": [
      "babel-core/register"
    ]
  }
}
